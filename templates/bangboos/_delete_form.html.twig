<form method="post" action="{{ path('app_bangboos_delete', {'id': bangboo.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ bangboo.id) }}">
    <div class="bg-red-600 flex items-center justify-center w-full p-8 my-8">
        <button class="btn">Supprimer le bangboo</button>
    </div>
</form>
