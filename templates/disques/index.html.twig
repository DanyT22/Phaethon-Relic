{% extends 'base.html.twig' %}

{% block title %}Disques index{% endblock %}

{% block body %}

    <div class="filtre"> </div>

    <a href="{{ path('app_disques_new') }}">Create new</a>

        {% for disque in disques %}
            <tr>
                <td>{{ disque.id }}</td>
                <td>{{ disque.emplacement }}</td>
                <td>{{ disque.mainStat }}</td>
                <td>{{ disque.subStat1 }}</td>
                <td>{{ disque.procSubStat1 }}</td>
                <td>{{ disque.subStat2 }}</td>
                <td>{{ disque.procSubStat2 }}</td>
                <td>{{ disque.subStat3 }}</td>
                <td>{{ disque.procSubStat3 }}</td>
                <td>{{ disque.subStat4 }}</td>
                <td>{{ disque.procSubStat4 }}</td>
                <td>
                    <a href="{{ path('app_disques_show', {'id': disque.id}) }}">show</a>
                    <a href="{{ path('app_disques_edit', {'id': disque.id}) }}">edit</a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}
